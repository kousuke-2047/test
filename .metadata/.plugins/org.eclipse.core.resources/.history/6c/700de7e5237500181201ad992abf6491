	$(".secondline").hide();
});
$(function(){
	$(".thirdline").hide();
});
$(function(){
	$(".fourthline").hide();
});
$(function(){
	$(".fifthline").hide();
});

$(function(){
	setTimeout(function(){
		$(".firstline").fadeIn(100);
	},200);
});

