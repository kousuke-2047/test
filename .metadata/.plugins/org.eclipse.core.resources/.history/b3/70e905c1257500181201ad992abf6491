$(document).ready(function(){
		$(".firstline").hide();
		$(".secondline").hide();
		$(".thirdline").hide();
		$(".fourthline").hide();
		$(".fifthline").hide();
});

$(function(){
	setTimeout(function(){
		$(".firstline").fadeIn(1000);
	},200);
});

$(function(){
	setTimeout(function(){
		$(".secondline").fadeIn(1000);
	},200);
});

$(function(){
	setTimeout(function(){
		$(".thirdline").fadeIn(1000);
	},200);
});

$(function(){
	setTimeout(function(){
		$(".fourthline").fadeIn(1000);
	},200);
});

$(function(){
	setTimeout(function(){
		$(".fifthline").fadeIn(1000);
	},200);
});


$(function(){
	setTimeout(function(){
		$(".firstline").animate({'marginLeft':'100px'
			});
	},1400);
});

$(function(){
	setTimeout(function(){
		$(".secondline").animate({'marginLeft':'100px'
			});
	},1600);
});

$(function(){
	setTimeout(function(){
		$(".thirdline").animate({'marginLeft':'100px'
			});
	},1800);
});

$(function(){
	setTimeout(function(){
		$(".fourthline").animate({'marginLeft':'100px'
			});
	},2000);
});

$(function(){
	setTimeout(function(){
		$(".fifthline").animate({'marginLeft':'100px'
			});
	},800);
});

