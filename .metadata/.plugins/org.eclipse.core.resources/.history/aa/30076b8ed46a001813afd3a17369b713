package com.rensyu.action;

import com.opensymphony.xwork2.ActionSupport;
import com.rensyu.dao.UserdeleteCDAO;
import java.sql.SQLException;





public class UserdeleteCAction extends ActionSupport{

	private UserdeleteCDAO dao = new UserdeleteCDAO();
	private String[] deleteusers;

	public String execute()throws SQLException{

		String result;

		if(deleteusers[0].equals(null)){
			result = ERROR;
		}else{

			dao.DeleteUser("deleteusers");



			result = SUCCESS;
		}

		return result;
	}







}
