package com.rensyu.action;

import com.opensymphony.xwork2.ActionSupport;
import com.rensyu.dao.UserCreateFDAO;
import java.sql.SQLException;

public class UserCreateFAction extends ActionSupport{

	public String createId;
	public String createPass;
	public String createName;

	UserCreateFDAO dao = new UserCreateFDAO();


	public String execute()throws SQLException{
		String result;

	if(createId==null || createPass==null || createName==null){
		result = ERROR;
	}else{

		dao.UserCreate(createId, createPass,createName);
		result = SUCCESS;
	}
	return result;



	}


}
