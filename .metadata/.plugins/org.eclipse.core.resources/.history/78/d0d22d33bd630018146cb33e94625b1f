
public class rensyu {

	public static void main(String[] args) {

		int selected =new java.util.Random().nextInt(4);
		switch(selected){
			case 1:
				System.out.println("検索");
				break;
			case 2:
				System.out.println("登録");
				break;
			case 3:
				System.out.println("削除");
				break;
			case 4:
				System.out.println("変更");
				break;
		}

	}

}
