$(document).ready(function(){
		$(".firstline").hide();
		$(".secondline").hide();
		$(".thirdline").hide();
		$(".fourthline").hide();
		$(".fifthline").hide();
});

$(function(){
	setTimeout(function(){
		$(".firstline").css("background-color","black");
	},300);
});

$(function(){
	setTimeout(function(){
		$(".secondline").css("background-color","black");
	},300);
});

$(function(){
	setTimeout(function(){
		$(".thirdline").css("background-color","black");
	},300);
});

$(function(){
	setTimeout(function(){
		$(".fourthline").css("background-color","black");
	},300);
});

$(function(){
	setTimeout(function(){
		$(".fifthline").css("background-color","black");
	},300);
});


$(function(){
	setTimeout(function(){
		$(".firstline").animate({'marginLeft':'100px'
			});
	},1000);
});

$(function(){
	setTimeout(function(){
		$(".secondline").animate({'marginLeft':'100px'
			});
	},1200);
});

$(function(){
	setTimeout(function(){
		$(".thirdline").animate({'marginLeft':'100px'
			});
	},1400);
});

$(function(){
	setTimeout(function(){
		$(".fourthline").animate({'marginLeft':'100px'
			});
	},1600);
});

$(function(){
	setTimeout(function(){
