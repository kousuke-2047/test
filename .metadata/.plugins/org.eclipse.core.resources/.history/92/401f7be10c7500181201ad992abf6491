$(function(){
	$('.firstline').animate({
		'marginleft':'500px'
	},1000);
});

$(function(){
	$(".secondline").animate({
		"right":"50"
	});
});

$(function(){
	$(".thirdline").animate({
		"left":"50"
	});
});

$(function(){
	$(".fourthline").animate({
		"right":"50"
	});
});

$(function(){
	$(".fifthline").animate({
		"left":"50"
	});
});